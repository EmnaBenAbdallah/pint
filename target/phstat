all: nc

include Makefile.common

SOURCES = \
	$(DIST_SOURCES) \
	$(BINDINGS_SOURCES)\
	$(PINTLIB_SOURCES) \
	$(PHLIB_SOURCES) \
	$(UI_SOURCES) \
	phstat.ml

RESULT = ph-stat

install:
	install -d $(DESTDIR)$(PREFIX)/bin
	install -m 0755 $(RESULT) $(DESTDIR)$(PREFIX)/bin

include OCamlMakefile
